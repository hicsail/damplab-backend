{
  "name": "elab-exploration",
  "version": "0.0.0",
  "description": "Series of scripts for testing eLab integration",
  "author": "cbolles @cbolles",
  "bin": {
    "elabx": "./bin/run"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/node": "^16.18.23",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "elabx",
    "dirname": "elabx",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "project": {
        "description": "Interact with eLab Project"
      },
      "auth": {
        "description": "Handle authentication logic"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/cbolles/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
